<% content_for(:title) { "LingSync" } %>
<div class="gridster">
  <ul>
  
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-view="Image" data-image="/lingsync.png" style="background-color:#74C2E1;"></div>
    </li>
  
	<li data-row="1" data-col="2" data-sizex="2" data-sizey="1">
      <div data-id="people_that_rock" data-view="People" data-title="People that Rock"></div>
      <i class="icon-user icon-background"></i>
    </li>
    
    
    
    <li data-row="2" data-col="1" data-sizex="2" data-sizey="2">
      <div data-id="fielddb_issues" data-view="Issues" style="background-color:#ff9618;" data-title="GitHub Issues"></div>
      <i class="icon-github icon-background"></i>
    </li>
    
    


<li data-row="2" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="loadavg1min" data-view="Graph" data-title="Server load" style="background-color:#47bbb3;" data-moreinfo="1 min intervals" ></div>
    </li>


    
    
    
    <li data-row="3" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="fielddb_commits" data-view="Commits" data-title="GitHub Commits"></div>
      <i class="icon-github icon-background"></i>
    </li>
    
    
    <li data-row="4" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="buzzwords" data-view="Text"></div>
    </li>
   
    <li data-row="4" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="activity" data-view="Activity" data-title="Server Activity" style="background-color:#96bf48;"></div>
    </li>
    
    
    <li data-row="5" data-col="2" data-sizex="3" data-sizey="1">
      <div data-id="fielddb_stats" data-view="Fielddbstats" data-title="LingSync Versions"></div>
    </li>
    
  </ul>
</div>